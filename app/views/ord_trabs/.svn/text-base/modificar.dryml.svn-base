<show-page>
  <parent-link:><a href="#{base_url}/ord_trabs">Atras</a></parent-link:>
  <heading:>
    <name/>&nbsp;&nbsp;&nbsp;Estado:&nbsp;<view:state/>
  </heading:>
  <content-body:>
      <error-messages/>
      <transition-buttons/>
      <table class="shower">
        <tr><td colspan="2">Cliente</td><td colspan="7"><view:producto.cliente/></td><td>Fecha:&nbsp;<view:fecha order="day,month,year"/></td><td>O.T.</td><td><view:numOT/></td></tr>
        <tr><td colspan="2">Producto</td><td colspan="8"><view:producto.name/></td><td>Numero de Factura</td><td><if test="&this.numFact"><view:numFact/></if></td></tr>
        <tr><td colspan="12"></td></tr>
        <tr><td colspan="3">Cod. Cliente</td><td colspan ="2"><view:producto.codCliente/></td><td colspan="4">Codigo de Producto</td><td colspan="3" class="centrada">Disposici&oacute;n Clisse</td></tr>
        <tr><td colspan="5">Solicitud a Ejecutar</td><td colspan="4">&nbsp;</td><td>Cilindro:&nbsp;<view:producto.cilindro/></td><td colspan="2">Ancho&nbsp;<view:producto.distorAncho/></td></tr>
        <tr><td colspan="5" style="font-size:80%"><view:visto/>&nbsp;V.B.&nbsp;&nbsp;<view:mtz/>&nbsp;MTZ&nbsp;&nbsp;<view:mtje/>&nbsp;MTJE&nbsp;&nbsp;<view:film/>&nbsp;Printer</td><td colspan="4">Medidas de Impresion</td><td style="font-size:80%">Distorsion Cil.&nbsp;<view:producto.cilindro.distorsion/></td><td colspan="2" style="font-size:80%;">% distorsion&nbsp;<view:producto.cilindro.pctDistor/></td></tr>
        <tr><td colspan="3">Mat. Prop.</td><td>Cant.</td><td>Dev.</td><td>Desarrollo</td><td style="border:solid;">&nbsp;</td><td>&nbsp;</td><td>Ancho</td><td>&nbsp;</td><td colspan="2">&nbsp;</td></tr>
        <tr><td><view:mpFTP style="font-size:70%"/></td><td colspan="2">Bajar de FTP</td><td><if test="&this.mpFTPq"><view:mpFTPq style="width:40px"/></if></td><td><view:mpFTPdev style="font-size:60%"/></td><td colspan="3">Codigo de Barras</td><td>Color</td><td colspan="3" class="centrada">Disposicion de bandas</td></tr>
        <tr><td><view:mpPel style="font-size:70%"/></td><td colspan="2">Peliculas</td><td><if test="&this.mpPelq"><view:mpPelq style="width:40px"/></if></td><td><view:mpPeldev style="font-size:60%"/></td><td colspan="3">Nº&nbsp;<view:producto.barcode/></td><td><view:producto.colorBarcode/></td><td colspan="3" rowspan="5" style="text-align: center"><if test="&this.producto.dispBandas"><view:producto.dispBandas/></if>&nbsp;(0=Bandas alineadas   1=Bandas desplazadas)</td></tr>
        <tr><td><view:mpImp style="font-size:70%"/></td><td colspan="2">Muestra Impresa</td><td><if test="&this.mpImpq"><view:mpImpq style="width:40px"/></if></td><td><view:mpImpdev style="font-size:60%"/></td><td colspan="3">Marcas en los Clisse</td><td>Colores</td></tr>
        <tr><td><view:mpMgr style="font-size:70%"/></td><td colspan="2">Material Gráfico</td><td><if test="&this.mpMgrq"><view:mpMgrq style="width:40px"/></if></td><td><view:mpMgrdev style="font-size:60%"/></td><td colspan="3"><view:mcGuia style="font-size:60%"/>&nbsp;Guia de corte</td><td><view:mcGuiacol style="width:40px"/></td></tr>
        <tr><td><view:mpOpt style="font-size:70%"/></td><td colspan="2">Mail/DVD/CD</td><td><if test="&this.mpOptq"><view:mpOptq style="width:40px"/></if></td><td><view:mpOptdev style="font-size:60%"/></td><td colspan="3"><view:mcMPunto style="font-size:60%"/>&nbsp;Micropunto x modulo</td><td><view:mcMPuntocol style="width:40px"/></td></tr>
        <tr><td><view:mpPtr style="font-size:70%"/></td><td colspan="2">Printer</td><td><if test="&this.mpPtrq"><view:mpPtrq style="width:40px"/></if></td><td><view:mpPtrdev style="font-size:60%"/></td><td colspan="3"><view:mcCruces style="font-size:60%"/>&nbsp;Cruces</td><td><view:mcCrucescol style="width:40px"/></td></tr>
        <tr><td colspan="5" class="centrada">Caracteristicas de Impresion</td><td colspan="3"><view:mcTacas style="font-size:60%"/>&nbsp;Tacas</td><td><view:mcTacascol style="width:40px"/></td><td colspan="3" class="centrada">Impresora: &nbsp;<view:producto.impresora/></td></tr>
        <tr><td colspan="5" rowspan="2"><view:producto.caractImp/></td><td colspan="3"><view:mcTiras style="font-size:60%"/>&nbsp;Tiras de control</td><td><view:mcTirascol style="width:40px"/></td><td colspan="3" class="centrada">N. de Pasos:&nbsp;<view:producto.nPasos/></td></tr>
        <tr><td colspan="3"><view:mcExceso style="font-size:60%"/>&nbsp;Exceso</td><td><view:mcExcesocol style="width:40px"/></td><td colspan="3" class="centrada">N. de Bandas&nbsp;<view:producto.nBandas/></td></tr>
        <tr><td colspan="5" class="centrada">Fotopolimero</td><td colspan="3"><view:mcMarcas style="font-size:60%"/>&nbsp;Marcas de calce</td><td><view:mcMarcascol style="width:40px"/></td><td colspan="3" rowspan="2"></td></tr>
        <tr><td colspan="5" class="centrada">&nbsp;</td><td colspan="3"><view:mcPimp style="font-size:60%"/>&nbsp;Guia de corte</td><td><view:mcPimpcol style="width:40px"/></td></tr>
        <tr><td colspan="12" class="centrada">Observaciones</td></tr>
        <tr><td colspan="12">&nbsp;<view:observaciones/></td></tr>
      </table>
    </content-body:>
</show-page>