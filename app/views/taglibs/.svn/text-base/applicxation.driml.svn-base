<include src="rapid" plugin="hobo"/>

<include src="taglibs/auto/rapid/cards"/>
<include src="taglibs/auto/rapid/pages"/>
<include src="taglibs/auto/rapid/forms"/>

<set-theme name="clean"/>

<def tag="app-name">Tecniflex</def>

<def tag="main-nav">
  <navigation class="main-nav" merge-attrs param="default">
    <nav-item href="#{base_url}/">Home</nav-item>
    <nav-item with="&Producto"><ht key="productos.nav_item">Productos</ht></nav-item>
    <nav-item with="&Cliente"><ht key="clientes.nav_item">Clientes</ht></nav-item>
    <nav-item with="&Contacto"><ht key="contactos.nav_item">Contactos</ht></nav-item>
    <nav-item with="&Impresora"><ht key="impresoras.nav_item">Impresoras</ht></nav-item>
    <nav-item with="&User"><ht key="users.nav_item">Usuarios</ht></nav-item>

  </navigation>
</def>

<extend tag="form" for ="Producto">
	<old-form merge>
		<error-messages param/>
		<b>Nombre</b>
		<input:name/>
		<b>Codigo Tecniflex</b>
		<input:codTflex/>
		<b>Codigo Cliente</b>
		<input:codCliente/><br/><br/>
		<h4><b>Medidas de Impresion</b></h4><br/>
		<b>Desarrollo</b>
		<input:mdi_desarrollo/>
		<b>Ancho</b>
		<input:mdi_ancho/><br/><br/>
		<b>Codigo de barras</b>
		<input:barcode/>
		<b>Color de c√≥digo</b>
		<input:colorBarcode/><br/><br/>
		<h4><b>Bandas</b></h4><br/>		
		<b>Disposicion</b>
		<input:dispBandas/><br/><br/>
		<h4><b>Distorsion</b></h4><br/>	
		<b>Total en el perimetro</b>
		<input:distTotalPerim/>
		<b>En el ancho</b>
		<input:distorAncho/><br/><br/>
		<b>Numero de pasos</b>
		<input:nPasos/>
		<b>Numero de Bandas</b>
		<input:nBandas/><br/><br/>
		<b>Color de Union</b>
		<input:colorUnion/>		
		<b>Superficie o reverso</b>
		<input:supRev/><br/><br/>
		<b>Caracteristicas de Impresion</b>
		<input:caractImp/><br/><br/>
		<br/><br/>
		<b>Ordenes de Trabajo</b>
		<input:ord_trabs/>
		<div param="actions">
			<submit label="Guardar" param/><or-cancel param="Cancelar" />
		</div>
	</old-form>
	</extend>
	

<!-- ====== Cilindro Pages ====== -->

<def tag="index-page" for="Cilindro">
  <page merge title="#{ht 'cilindros.index.title', :default=>['Cilindros'] }">
    <body: class="index-page cilindro" param/>

    <content: param>
      <header param="content-header">
        <h2 param="heading">
          <ht key="cilindros.index.heading">
            Cilindros
          </ht>
        </h2>

        <p param="count" if>
          <ht key="cilindros.collection.count" count="&this.size">
            Hay <count />
          </ht>
        </p>
      </header>

      <section param="content-body">
        <a action="new" to="&model" param="new-link">
          <ht key="cilindros.actions.new">New Cilindro</ht>
        </a>

        <page-nav param="top-page-nav"/>

        <collection param/>

        <page-nav param="bottom-page-nav"/>


      </section>
    </content:>
  </page>
</def>


<def tag="new-page" for="Cilindro">
  <page merge title="#{ht 'cilindros.new.title', :default=>[' New Cilindro'] }">
    <body: class="new-page cilindro" param/>

    <content: param>
      <section param="content-header">
        <h2 param="heading">
          <ht key="cilindros.new.heading">
            Nuevo Cilindro
          </ht>
        </h2>
      </section>

      <section param="content-body">
        <form param>
          <submit: label="#{ht 'cilindros.actions.create', :default=>['Create Cilindro']}"/>
        </form>
      </section>
    </content:>
  </page>
</def>


<def tag="show-page" for="Cilindro">
  <page merge title="#{ht 'cilindros.show.title', :default=>['Cilindro'] }">

    <body: class="show-page cilindro" param/>

    <content: param>
          <header param="content-header">
            <a:impresora param="parent-link">&laquo; <ht key="cilindros.actions.back" to="impresora"><name/></ht></a:impresora>
            <h2 param="heading">
              <ht key="cilindros.show.heading" name="&this.respond_to?(:name) ? this.name : ''">
                <name/>
              </ht>
            </h2>

            <record-flags fields="" param/>

            <a action="edit" if="&can_edit?" param="edit-link">
              <ht key="cilindros.actions.edit" name="&this.respond_to?(:name) ? this.name : ''">
                Editar Cilindro
              </ht>
            </a>
          </header>

          <section param="content-body">
            <field-list fields="distorsion, pctDistor, anilox, lineatura, duplo, espesor" param/>
          </section>
    </content:>

  </page>
</def>


<def tag="edit-page" for="Cilindro">
  <page merge title="#{ht 'cilindros.edit.title', :default=>['Edit Cilindro'] }">

    <body: class="edit-page cilindro" param/>

    <content:>
      <section param="content-header">
        <h2 param="heading">
          <ht key="cilindros.edit.heading" name="&this.respond_to?(:name) ? this.name : ''">
            Editar <type-name/>
          </ht>
        </h2>
        <delete-button label="#{ht 'cilindros.actions.delete', :default=>['Remove This Cilindro']}" param/>
      </section>

      <section param="content-body">
        <form param/>
      </section>
    </content:>

  </page>
</def>








