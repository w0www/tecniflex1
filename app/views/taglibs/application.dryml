        <tr><td>Cajas Abiertas</td><td><% if @uniqsdos.first %><%= @uniqsdos.first.existalto.to_f -%><%end%></td><td><%if @uniqsdos.first %><%= @uniqsdos.first.existancho.to_f -%><%end%></td><td><%= @cuniqsdos.values.first -%></td><td/><td/></tr>
