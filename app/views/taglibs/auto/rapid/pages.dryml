<def tag="index-page" for="Tinta">
  <page merge title="#{ht 'tintas.index.title', :default=>['Tintas'] }">
    <body: class="index-page tinta" param/>

    <content: param>
      <header param="content-header">
        <h2 param="heading">
          <ht key="tintas.index.heading">
            Tintas
          </ht>
        </h2>

        <p param="count" if>
          <ht key="tintas.collection.count" count="&this.size">
            There <count prefix="are"/>
          </ht>
        </p>
      </header>

      <section param="content-body">
        <a action="new" to="&model" param="new-link">
          <ht key="tintas.actions.new">New Tinta</ht>
        </a>

        <page-nav param="top-page-nav"/>

        <collection param/>

        <page-nav param="bottom-page-nav"/>


      </section>
    </content:>
  </page>
</def>


<def tag="new-page" for="Tinta">
  <page merge title="#{ht 'tintas.new.title', :default=>[' New Tinta'] }">
    <body: class="new-page tinta" param/>

    <content: param>
      <section param="content-header">
        <h2 param="heading">
          <ht key="tintas.new.heading">
            New Tinta
          </ht>
        </h2>
      </section>

      <section param="content-body">
        <form param>
          <submit: label="#{ht 'tintas.actions.create', :default=>['Create Tinta']}"/>
        </form>
      </section>
    </content:>
  </page>
</def>


<def tag="show-page" for="Tinta">
  <page merge title="#{ht 'tintas.show.title', :default=>['Tinta'] }">

    <body: class="show-page tinta" param/>

    <content: param>
          <header param="content-header">
            <h2 param="heading">
              <ht key="tintas.show.heading" name="&this.respond_to?(:name) ? this.name : ''">
                <name/>
              </ht>
            </h2>

            <record-flags fields="" param/>

            <a action="edit" if="&can_edit?" param="edit-link">
              <ht key="tintas.actions.edit" name="&this.respond_to?(:name) ? this.name : ''">
                Edit Tinta
              </ht>
            </a>
          </header>

          <section param="content-body">
            <field-list fields="posicion, anilox, lpi, ord_trab" param/>
          </section>
    </content:>

  </page>
</def>


